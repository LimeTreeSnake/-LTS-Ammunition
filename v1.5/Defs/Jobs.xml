            <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
        </JobDef>
        <JobDef>
            <defName>LTS_LootAmmo</defName>
            <driverClass>Ammunition.JobDrivers.JobDriver_LootAmmo</driverClass>
            <reportString>Looting ammo from TargetA.</reportString>
            <taleOnCompletion>LTS_Looted</taleOnCompletion>
            <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
        </JobDef>
