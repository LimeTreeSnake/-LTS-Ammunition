<?xml version="1.0" encoding="utf-8" ?>
    <Defs>
        <!--==================== Small ======================== -->
        <ThingDef ParentName="PackSmallBase">
            <defName>KitPrimitiveSmall</defName>
            <label>small primitive kit</label>
            <description>A small kit for carrying primitive munition.</description>
            <graphicData>
                <texPath>Kits/PrimitiveBag_0</texPath>
                <graphicClass>Graphic_Single</graphicClass>
            </graphicData>
            <techLevel>Neolithic</techLevel>
            <stuffCategories>
                <li>Woody</li>
            </stuffCategories>
            <comps>
                <li Class="Ammunition.Components.CompProps_Kit">
                    <ammoCapacity>60</ammoCapacity>
                    <kitCategory>Primitive</kitCategory>
                </li>
            </comps>
        </ThingDef>
